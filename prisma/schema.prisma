//   message    String
//   date       DateTime
//   created_at DateTime @default(now())
//   updated_at DateTime @updatedAt
// }

model Appointment {
  idAppointment String   @id @default(uuid())
  title         String
  message       String
  date          DateTime
  created_at    DateTime @default(now())
  updated_at    DateTime @updatedAt
}

