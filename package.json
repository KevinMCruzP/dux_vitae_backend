    "@types/jsonwebtoken": "^8.5.1",
    "@types/multer": "^1.4.7",
    "@types/uuid": "^8.3.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "husky": "^8.0.1",
    "prisma": "^3.14.0",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@mermaid-js/mermaid-cli": "^9.1.2",
    "@prisma/client": "^3.14.0",
    "@prisma/generator-helper": "^3.15.0-integration-tweak-ci.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "multer": "^1.4.5-lts.1",
    "prisma-erd-generator": "^0.11.4",
    "socket.io": "^4.5.1",
    "uuid": "^8.3.2"
  },
  "config": {
    "commitizen": {
